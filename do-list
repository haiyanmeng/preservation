       -a, --all
       -s, --kernel-name
       -n, --nodename
       -r, --kernel-release
              print the kernel release
       -v, --kernel-version
              print the kernel version
       -m, --machine
              print the machine hardware name
       -p, --processor
       -i, --hardware-platform
              print the hardware platform or "unknown"
       -o, --operating-system
              print the operating system

